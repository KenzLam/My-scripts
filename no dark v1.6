	Open.Visible = false
	Main.Visible = true
	end
	Ticker = tick()
end)


local Lighting = game:GetService("Lighting")

local FoundAtmo = Lighting:FindFirstChildOfClass("Atmosphere")
local FoundBloom = Lighting:FindFirstChildOfClass("BloomEffect")
local FoundBlur = Lighting:FindFirstChildOfClass("BlurEffect")
local FoundColorCor = Lighting:FindFirstChildOfClass("ColorCorrectionEffect")
local FoundDepthOfField = Lighting:FindFirstChildOfClass("DepthOfFieldEffect")
local FoundSunRays = Lighting:FindFirstChildOfClass("SunRaysEffect")

local function SetLighting()
	Lighting.Ambient = Color3.fromRGB(255, 255, 255)
	Lighting.Brightness = _G.ND_Brightness
	Lighting.ColorShift_Bottom = Color3.fromRGB(255, 255, 255)
	Lighting.ColorShift_Top = Color3.fromRGB(255, 255, 255)
	Lighting.GlobalShadows = false
	Lighting.OutdoorAmbient = Color3.fromRGB(255, 255, 255)
	Lighting.ExposureCompensation = _G.ND_ExposureCompensation

	if FoundAtmo then
		FoundAtmo.Density = 0
		FoundAtmo.Offset = 0
		FoundAtmo.Glare = 0
		FoundAtmo.Haze = 0
	else
		Lighting.FogColor = Color3.fromRGB(255, 255, 255)
		Lighting.FogEnd = math.huge
		Lighting.FogStart = 0
	end

	if FoundBloom then
		FoundBloom.Enabled = false
	end

	if FoundBlur then
		FoundBlur.Enabled = false
	end

	if FoundColorCor then
		FoundColorCor.Enabled = false
	end

	if FoundDepthOfField then
		FoundDepthOfField.Enabled = false
	end

	if FoundSunRays then
		FoundSunRays.Enabled = false
	end
end

local function Enabled()
	wait()
	if _G.ND_Enable == true then

		SetLighting()

	end
end


Button.MouseButton1Click:Connect(function()
	if _G.ND_Enable == false then
		_G.ND_Enable = true
		Info.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
		SetLighting()
	else
		_G.ND_Enable = false
		Info.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
end)

Button_2.MouseButton1Click:Connect(function()
	if _G.ND_Loop == false then
		_G.ND_Loop = true
		Info_2.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
	else
		_G.ND_Loop = false
		Info_2.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
end)


Lighting.Changed:Connect(function()
	if _G.ND_Enable == true and _G.ND_Loop == true then
		SetLighting()
	end
end)


TextBox:GetPropertyChangedSignal("Text"):Connect(function()
	if tonumber(TextBox.Text) ~= nil then

		_G.ND_Brightness = tonumber(TextBox.Text)

		if _G.ND_Loop == true and _G.ND_Enable == true then
			SetLighting()
		end

	end
end)

TextBox_2:GetPropertyChangedSignal("Text"):Connect(function()
	if tonumber(TextBox_2.Text) ~= nil then

		_G.ND_ExposureCompensation = tonumber(TextBox_2.Text)

		if _G.ND_Loop == true and _G.ND_Enable == true then
			SetLighting()
		end

	end
end)

